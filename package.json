{
  "name": "polaris-file-explorer",
  "version": "0.1.0",
  "private": false,
  "author": "Arthur Wallendorff - Metaoist Dsgn",
  "license": "ISC",
  "homepage": "https://github.com/ZyriabDsgn/polaris-file-explorer#readme",
  "bug": {
    "url": "https://github.com/ZyriabDsgn/polaris-file-explorer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZyriabDsgn/polaris-file-explorer.git"
  },
  "keywords": [
    "react",
    "typescript",
    "polaris",
    "shopify",
    "file explorer"
  ],
  "main": "dist/FileExplorer.js",
  "module": "dist/FileExplorer.js",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@auth0/auth0-react": "^1.11.0",
    "@shopify/polaris": "^10.4.1",
    "@shopify/polaris-icons": "^6.4.0",
    "normalize-path": "^3.0.0",
    "papaparse": "^5.3.2",
    "react": "^17.0.2",
    "react-i18next": "^11.18.6",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "publish:npm": "rm -rf dist && mkdir dist && babel ./src/components -d dist --copy-files",
    "publish:npm:win": "(if exist dist rmdir /s /q dist) && mkdir dist && babel ./src/components -d dist --copy-files"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^16.11.64",
    "@types/normalize-path": "^3.0.0",
    "@types/papaparse": "^5.3.5",
    "@types/react": "^18.0.21"
  }
}
